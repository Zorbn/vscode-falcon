	"tokenColors": [
		{
			"scope": [
				"constant",
				"keyword.other.unit"
			],
			"settings": {
				"foreground": "#B4B4B9",
				"fontStyle": "italic",
			}
		},
		{
			"scope": [
				"variable"
			],
			"settings": {
				"foreground": "#99A4BC"
			}
		},
		{
			"scope": [
				"storage.type",
				"storage.modifier",
				"entity.name.type",
			],
			"settings": {
				"foreground": "#DFDFE5"
			}
		},
		{
			"scope": [
				"keyword",
			],
			"settings": {
				"foreground": "#FFC552"
			}
		},
		{
			"scope": [
				"keyword.operator",
				"constant.character.escape",
			],
			"settings": {
				"foreground": "#FF761A"
			}
		},
		{
			"scope": [
				"punctuation",
			],
			"settings": {
				"foreground": "#DFDFE5"
			}
		},
		{
			"scope": [
				"punctuation.definition.string",
				"string",
			],
			"settings": {
				"foreground": "#C8D0E3"
			}
		},
		{
			"scope": [
				"keyword.control.directive",
				"punctuation.definition.directive",
			],
			"settings": {
				"foreground": "#CFC1B2"
			}
		},
		{
			"scope": [
				"comment"
			],
			"settings": {
				"foreground": "#787882",
				"fontStyle": "italic",
			}
		}
	]
}